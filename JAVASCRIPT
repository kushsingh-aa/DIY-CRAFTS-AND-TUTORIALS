// ====== Tutorial Data ======
const tutorials = [
    {
        title: "Paper Flower",
        desc: "Beautiful decorative flowers using paper.",
        img: "data:image/jpeg;base64,",
        steps: ["Cut paper", "Make petals", "Arrange layers", "Glue and finish"]
    },
    {
        title: "DIY Photo Frame",
        desc: "Handmade frame for photos.",
        img: "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTHJSly76b9FqVUJbD07Ex5I9tXjmfafvp17A&s",
        steps: ["Cut cardboard", "Decorate", "Attach support", "Insert photo"]
    },
    {
        title: "Glass Jar Lantern",
        desc: "Convert jars into lanterns.",
        img: "data:image/jpeg;base64,",
        steps: ["Clean jar", "Paint", "Wrap rope", "Put LED inside"]
    }
];

// ====== LOAD TUTORIAL CARDS ======
function loadTutorials() {
    const box = document.getElementById("cardBox");
    if (!box) return;

    tutorials.forEach((t, i) => {
        box.innerHTML += `
            <div class="card">
                <img src="${t.img}">
                <h3>${t.title}</h3>
                <p>${t.desc}</p>
                <div class="btn" onclick="openTutorial(${i})">View</div>
            </div>
        `;
    });
}

// ====== MODAL ======
function openTutorial(i) {
    const modal = document.getElementById("modal");
    const title = document.getElementById("modalTitle");
    const steps = document.getElementById("modalSteps");

    title.innerHTML = tutorials[i].title;
    steps.innerHTML = tutorials[i].steps.map(s => `<li>${s}</li>`).join("");

    modal.style.display = "flex";
}

function closeModal() {
    document.getElementById("modal").style.display = "none";
}

// ====== LOGIN VALIDATION ======
function validateLogin() {
    let email = document.getElementById("email");
    let pass = document.getElementById("password");
    let error = document.getElementById("error");

    if (email.value === "" || pass.value === "") {
        error.innerHTML = "All fields are required!";
        return false;
    }

    if (!email.value.includes("@")) {
        error.innerHTML = "Enter valid email!";
        return false;
    }

    if (pass.value.length < 5) {
        error.innerHTML = "Password must be 5+ characters!";
        return false;
    }

    alert("Login Successful!");
    return true;
}


